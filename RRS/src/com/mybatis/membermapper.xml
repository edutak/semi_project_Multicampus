	<insert id="insert" parameterType="member">
		INSERT INTO MEMBER VALUES (#{userid},#{userpwd},#{username},#{userphonenumber},#{nickname},#{admincheck})
	</insert>
	
	<update id="update" parameterType="member">
		UPDATE MEMBER SET PWD=#{pwd}, NAME=#{name} WHERE ID=#{userid}
	</update>
	
	<delete id="delete" parameterType="String">
		DELETE FROM MEMBER WHERE ID=#{k}
	</delete>
	
	<select id="select" parameterType="String" 
	resultType="member">
		SELECT * FROM MEMBER WHERE ID=#{k}
	</select>
	
	<select id="selectall" resultType="member">
		SELECT * FROM MEMBER 
	</select>
	
